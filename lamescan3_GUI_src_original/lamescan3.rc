        BEGIN
            MENUITEM "Список диапазонов\tDel",      IDC_REMOVE_RANGE
            MENUITEM SEPARATOR
            MENUITEM "Все ""Просканированные""",    IDC_REMOVE_UP
            MENUITEM "Все ""Не просканированные""", IDC_REMOVE_QUEUE
