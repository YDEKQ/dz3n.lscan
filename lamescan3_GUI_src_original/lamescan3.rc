        MENUITEM "Основное\tF9",                IDC_SCANNER_SETUP
        MENUITEM "Интерфейс\tShift+F9",         IDC_CUSTOMIZE
        MENUITEM "Прокси-сервер\tCtrl+P",       IDC_PROXY_SETUP
